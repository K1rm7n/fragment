{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "concurrently \"yarn api:dev\" \"yarn client:dev\"",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "allupdate": "yarn upgrade-interactive --latest",
    "client:dev": "nuxt dev --dotenv .env.local",
    "client:start": "node .output/server/index.mjs",
    "api:dev": "CONFIG_PATH=\".env.local\" npx directus start",
    "api:dev:install": "CONFIG_PATH=\".env.local\" npx directus database install && CONFIG_PATH=\".env.local\" npx directus bootstrap",
    "api:dev:update": "CONFIG_PATH=\".env.local\" npx directus database migrate:latest",
    "api:start": "npx directus start",
    "api:install": "npx directus database install && npx directus bootstrap",
    "api:update": "npx directus database migrate:latest",
    "start": "concurrently \"yarn api:start\" \"yarn client:start\"",
    "prod": "yarn client:start & yarn api:start"
  },
  "dependencies": {
    "@nuxtjs/seo": "^2.0.0-rc.18",
    "concurrently": "^8.2.2",
    "curtainsjs": "^8.1.6",
