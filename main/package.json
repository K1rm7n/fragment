    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-perfectionist": "^1.5.1",
    "eslint-plugin-prettier": "^5.0.0",
    "gsap": "^3.12.5",
    "nuxt-headlessui": "^1.2.0",
    "nuxt-icon": "^1.0.0-beta.7",
    "nuxt-security": "^2.0.0-rc.9",
    "nuxt-swiper": "^1.2.2",
