    "directus-extension-board-layout": "^1.1.2",
    "directus-extension-wpslug-interface": "^1.1.0",
    "nuxt": "^3.12.4",
    "pg": "^8.12.0",
    "tailwindcss": "^3.4.7",
    "vue": "latest"
  },
  "devDependencies": {
    "@directus/sdk": "^17.0.0",
    "@google/model-viewer": "3.1.1",
    "@intlify/core-base": "^9.2.0",
